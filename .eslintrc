{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 9,
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "prettier/prettier": "error"
    }
}